{
    "manifest_version": 3,
    "name": "Instagram Spam Filter",
    "description": "Detect spam comments on Instagram",
    "version": "1.0.0",
    "icons": {"128": "images/logo.png"},
    "action":{
        "default_icon": "images/logo.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab", "tabs", "http://127.0.0.1:5000"]
}